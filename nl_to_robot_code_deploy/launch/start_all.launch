<launch>
    <!-- Jackal autonomy stack -->
    <include file="$(find ut_jackal)/launch/autonomy.launch" />

    <node name="interface_server"
        pkg="nl_to_robot_code_deploy"
        type="interface_server.py"
        cwd="node"
        args="--ip 10.0.0.123 --port 8190"
        respawn="true"
        respawn_delay="2"
        output="screen"/>

    <node name="gui"
        pkg="nl_to_robot_code_deploy"
        type="gui.py"
        cwd="node"
        respawn="true"
        respawn_delay="2"
        output="screen"/>

    <node name="interface"
        pkg="nl_to_robot_code_deploy"
        type="interface.py"
        cwd="node"
        respawn="true"
        respawn_delay="2"
        output="screen"/>
</launch>
